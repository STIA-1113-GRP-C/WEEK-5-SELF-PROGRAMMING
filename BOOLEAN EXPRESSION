package selfProgramming;
import java.util.Scanner;
public class booleanExpression {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int num1, num2, num3, total;
		
		System.out.print("Enter the first number: ");
		num1 = sc.nextInt();
		System.out.print("Enter the second number: ");
		num2 = sc.nextInt();
		System.out.print("Enter the third number: ");
		num3 = sc.nextInt();
		
		total = num1 + num2 + num3;
		
		System.out.println();
		
		if (total == 10)// == maksudnya sama dengan
			System.out.println("The total is 10");// betul
		else
			System.out.println("The total is not 10");//salah
		
		if (total != 20)// != maksudnya tidak sama dengan
			System.out.println("The total is not 20");//betul
		else
			System.out.println("The total is 20");//salah
		
		if (total < 15)// < maksudnya kurang dari
			System.out.println("The total is less than 15");//betul
		else
			System.out.println("The total is greater than 15");//salah
		
		if (total > 5)// > maksudnya lebih dari
			System.out.println("The total is greater than 5");//betul
		else
			System.out.println("The total is less than 5");//salah
		
		if (total <= 7)// <= maksudnya kurang dari atau sama dengan
			System.out.println("The total is less than or equal to 7");//betul
		else
			System.out.println("The total is greater than or not equal to 7");//salah
		
		if (total >= 9)// >= maksudnya besar dari atau sama dengan
			System.out.println("The total is greater than or equal to 9");//betul
		else
			System.out.println("The total is less than or not equal to 9");//salah

	}

}
